{
  "lint-staged": {
    "files": [".husky/_/.gitignore", ".husky/_/husky.sh", ".husky/pre-commit"],
    "pre-commit": {
      "filePath": ".husky/pre-commit",
      "content": ["\n . \"$(dirname \"$0\")/_/husky.sh\" \n npx lint-staged \n"]
    }
  },
  "commitlint": {
    "files": [".husky/_/.gitignore", ".husky/_/husky.sh", ".husky/commit-msg"],
    "commitmsg": {
      "filePath": ".husky/commit-msg",
      "content": ["npx --no -- commitlint --edit $1"]
    }
  },
  "jest": {
    "files": [".husky/_/.gitignore", ".husky/_/husky.sh", ".husky/pre-commit"],
    "pre-commit": { "filePath": ".husky/pre-commit", "content": [] }
  }
}
